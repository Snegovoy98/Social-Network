{% extends 'base.html.twig' %}


{% block title %}Social Network{% endblock %}
{% block stylesheets %}
<link  href="{{ asset('images/favicon.ico') }}" rel="shortcut icon">
<link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet">
   <link href="{{ asset('css/font-awesome.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/nprogress.css') }}" rel="stylesheet">
    <link href="{{ asset('css/daterangepicker.css') }}" rel="stylesheet">
    <link href="{{ asset('css/custom.min.css') }}" rel="stylesheet">
{% endblock %}

{% block body%}
    {% block nav %}
<div class="container body">
      <div class="main_container">
        <div class="col-md-3 left_col">
          <div class="left_col scroll-view">
            <div class="navbar nav_title" style="border: 0;">
            <div class="profile_pic">
                <img src="images/social-network.jpg" alt="..." class="img-thumbnail profile_img">
              </div>
            </div>
            <div class="clearfix"></div>
    {% endblock %}
    {% block left_sidebar %}
         <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
              <div class="menu_section">
                <ul class="nav side-menu">
                  <li><a><i class="fa fa-home"></i> Моя страница</a></li>
                  <li><a><i class="fa fa-home"></i>Новости</a></li>
                  <li><a><i class="fa fa-home"></i>Сообщения</a></li>
                  <li><a><i class="fa fa-home"></i>Друзья</a></li>
                  <li><a><i class="fa fa-clone"></i>Аудиозаписи </a></li>
                </ul>
              </div>
              <div class="menu_section"></div>
            </div>
           <div class="sidebar-footer hidden-small">
              <a data-toggle="tooltip" data-placement="top" title="Settings">
                <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="Lock">
                <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="Logout" href="login.html">
                <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
              </a>
            </div>
          </div>
        </div>

    {% endblock %}
    {% block top_sidebar %}
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Social Network</h3>
              </div>

              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                    <li><a href="{{ path('authorization_page') }}"> Log Out</a></li>
                  </div>
                </div>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_content">
                    <div class="col-md-3 col-sm-3 col-xs-12 profile_left">
                      <div class="profile_img">
                        <div id="crop-avatar" class=" col-md-12 col-sm-12 col-xs-12">
                          <img class="img-responsive avatar-view col-md-12 col-sm-12 col-xs-12" src="images/developer.svg" alt="Avatar" title="Change the avatar">
                        </div>
                      </div>

                      <a class="btn btn-success">Редактировать</a>
                      <br />
                    </div>
                    <div class="col-md-9 col-sm-7 col-xs-12 align-content-md-center">
                           <h3 class="text-center">Samuel Doe</h3>
                    </div>
                    <div class="col-md-8 col-sm-5 col-xs-12">
                        <span class="center-margin">Дата рождения:</span>
                   </div>
                </div>
              </div>
            </div>
          </div>
          <div class="x_panel col-md-6 col-sm-5 col-xs-5 align-content-md">
          <div class="x_content col-md-5 col-sm-7 col-xs-12 align-content-md">
          <div class="col-md-5 col-sm-7 col-xs-12 align-content-md-right">
           <h3 class="text-center">Друзья</h3>
           </div>
          </div>
          </div>
          <div class="x_panel col-md-6 col-sm-5 col-xs-5 align-content-md-center">
            <div class="x_content col-md-5 col-sm-7 col-xs-12 align-content-md-center">
            <div class="col-md-5 col-sm-7 col-xs-12 align-content-md-center">
              <h3 class="text-center">Аудиозаписи</h3>
               </div>
            </div>
            </div>
        </div>
{% endblock %}

{% block footer %}

{% endblock %}
{% endblock %}
{% block javascripts %}
   <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/fastclick.js') }}"></script>
    <script src="{{ asset('js/nprogress.js') }}"></script>
    <script src="{{ asset('raphael.min.js') }}"></script>
    <script src="{{ asset('js/morris.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap-progressbar.min.js') }}"></script>
    <script src="{{ asset('js/moment.min.js') }}"></script>
    <script src="{{ asset('js/daterangepicker.js') }}"></script>
    <script src="{{ asset('js/custom.min.js') }}"></script>
{% endblock %}